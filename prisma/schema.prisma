// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            int       @default(cuid()) @id
  createTime    DateTime  @default(now()) @map(name: "createTime")
  updateTime    DateTime  @updatedAt @map(name: "updateTime")
  ownerid       int?
  ownerName     String?
  email         String?
  password      String?
  @@map(name: "users")
}